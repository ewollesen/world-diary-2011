.changes

  .left
    %h2
      %span Rev #{@rev_left+1} (#{date_left})
    .contents
      %ol
        = list_of(output_left) do |line|
          = line

  .right
    %h2
      %span Rev #{@rev_right+1} (#{date_right})
    .contents
      %ol
        = list_of(output_right) do |line|
          = line

  .clear
